AWE=$(echo awk -F"[,:]" '{for(i=1;i<=NF;i++){if($i~/id\042/){print$(i+1)}}}')
WID=$(awk -F"[,:]" '{for(i=1;i<=NF;i++){if($i~/weather\042/){print$(i+2)}}}' ~/.brweather/cache/open1)

D1=$(echo grep -m1 $(date +%a) ~/.brweather/cache/open2)
WID1=$(echo $($D1 | $AWE))

