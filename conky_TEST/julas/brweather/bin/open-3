#Dia/NOITE. Condições atuais
case `date "+%H"` in
  "18") NOITE $*
;;
  "19") NOITE $*
;;
  "20") NOITE $*
;;
  "21") NOITE $*
;;
  "22") NOITE $*
;;
  "23") NOITE $*
;;
  "00") NOITE $*
;;
  "01") NOITE $*
;;
  "02") NOITE $*
;;
  "03") NOITE $*
;;
  "04") NOITE $*
;;
  "05") NOITE $*
;;
  "06") DIA $*
;;
  "07") DIA $*
;;
  "08") DIA $*
;;
  "09") DIA $*
;;
  "10") DIA $*
;;
  "11") DIA $*
;;
  "12") DIA $*
;;
  "13") DIA $*
;;
  "14") DIA $*
;;
  "15") DIA $*
;;
  "16") DIA $*
;;
  "17") DIA $*
;;
  *) echo "" >/dev/null
;;
esac

