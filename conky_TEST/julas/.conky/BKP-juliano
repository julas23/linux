KERNEL: ${goto 85}${color yellow}${kernel}${color}
ENVIRONMENT: ${goto 85}${color yellow}${exec myconky env}${color}
UPTIME: ${goto 85}${color yellow}${uptime}${color}
CPU:${color yellow}${exec myconky cpu}${color}
Rotacao:${color blue}${goto 100}${exec echo 'WaterCooled'}${color}
Temperatura:${goto 90}CPU ${color yellow}${exec sensors |grep Tctl |awk '{print $2}'}${goto 175}${color}MB ${color yellow}${exec sensors |grep edge |awk '{print $2}'}${color}
${color yellow}${cpu cpu0}%${goto 50}${color}${cpubar cpu0 10,220}${color}
${color yellow}${freq_g 0}Mhz${goto 50}${color}${cpugraph cpu0 10,220 859900 DC322F scale -t}${color}
RAM: ${goto 45}${color yellow}${memmax}${color}
USO: ${goto 45}${color yellow}${memperc}%${color}
USO: ${goto 45}${color yellow}${mem}${color}
LIVRES: ${goto 45}${color yellow}${memfree}${color}
${voffset -53}${goto 110}Consumo
${goto 110}${membar 10,335}
${goto 110}Realtime
${goto 110}${memgraph 10,335 859900 DC322F scale -t}
${goto 5}sda - / ${fs_bar 10,58 /} ${color yellow} ${fs_free /} ${color}Free
${goto 5}R: ${diskio_read /dev/sda}${goto 100}W: ${diskio_write /dev/sda} ${goto 180}IOPs: ${diskio /dev/sda}
${goto 5}${diskiograph /dev/sda 10,250 859900 DC322F scale -t}
${goto 5}Raid1 - /home ${fs_bar 10,58 /home} ${color yellow} ${fs_free /home} ${color}Free
${goto 5}R: ${diskio_read /dev/vg_raid1/lv_raid1}${goto 100}W: ${diskio_write /dev/vg_raid1/lv_raid1} ${goto 180}IOPs: ${diskio /dev/vg_raid1/lv_raid1}
${goto 5}${diskiograph /dev/vg_raid1/lv_raid1 10,250 859900 DC322F scale -t}
${if_existing /sys/class/net/enp5s0/operstate up}
${goto 0}LAN:${goto 28}${color yellow}${if_existing /proc/net/route enp5s0}${addr enp5s0}${else}Desconectado${endif}${color}${goto 120}WAN:${goto 152}${color yellow}${if_existing /proc/net/route enp5s0}${exec curl -s meuip.com.br |grep "Meu ip é" |awk '{print $8}' |cut -d'<' -f 1}${else}Offline${endif}${color}
${goto 0}UP:${goto 20}${upspeedgraph enp5s0 10,58 859900 DC322F scale -t}${goto 85} ${upspeed enp5s0} ${goto 128}DL:${goto 148}${downspeedgraph enp5s0 10,58 859900 DC322F scale -t}${goto 205} ${downspeed enp5s0}
${else}${if_existing /sys/class/net/wlp4s0/operstate up}
${goto 0}WLAN:${goto 36}${color yellow}${if_existing /proc/net/route wlp4s0}${addr wlp4s0}${else}Desconectado${endif}${color}${goto 120}WAN:${goto 152}${color yellow}${if_existing /proc/net/route wlp4s0}${exec curl -s meuip.com.br |grep "Meu ip é" |awk '{print $8}' |cut -d'<' -f 1}${else}Offline${endif}${color}
${goto 0}UP:${goto 20}${upspeedgraph wlp4s0 10,58 859900 DC322F scale -t}${goto 85} ${upspeed wlp4s0} ${goto 128}DL:${goto 148}${downspeedgraph wlp4s0 10,58 859900 DC322F scale -t}${goto 205} ${downspeed wlp4s0}

