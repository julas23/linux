#BATT 1: ${goto 85}${color}${battery_bar 8,50 BAT1} ${color yellow}${battery_percent BAT1}% ${goto 175}${battery BAT1}${color}
#${else}${endif}${endif}${voffset 6}${goto 0}Status da VPN: ${goto 100} ${if_existing /sys/class/net/tunsnx/operstate unknown}(${color yellow}${addr tunsnx}${color})${else}(${color yellow}${addr tunsnx}${color})${endif}
################## HOJE
${voffset 5}${alignr}${font Courier:pixelsize=56}${time %H:%M}${font}
